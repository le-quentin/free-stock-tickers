{
  "name": "free-stock-tickers",
  "version": "1.0.0",
  "description": "Freely serve live stock information by scraping web pages",
  "type": "module",
  "main": "src/index.ts",
  "imports": {
    "#free-stock-tickers/*": {
      "node": "./built/src/*"
    },
    "#test/*": {
      "node": "./built/test/*"
    }
  },
  "scripts": {
    "test": "c8 --all --src=built/src --exclude=built/test --exclude=**/index.js ava",
    "build": "tsc",
    "clean": "rm -rf ./built",
    "start": "node built/src/index.js",
    "watch": "tsc -w & nodemon -q -w built built/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/le-quentin/free-stock-tickers.git"
  },
  "author": "le-quentin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/le-quentin/free-stock-tickers/issues"
  },
  "homepage": "https://github.com/le-quentin/free-stock-tickers#readme",
  "dependencies": {
    "axios": "^1.3.3",
    "cheerio": "^1.0.0-rc.12",
    "csv-stringify": "^6.2.4",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/cheerio": "^0.22.31",
    "@types/express": "^4.17.17",
    "@types/node": "18.14.0",
    "@types/sinon": "^10.0.13",
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "csv-parse": "^5.3.5",
    "nodemon": "^2.0.20",
    "sinon": "^15.0.1",
    "typescript": "^4.9.5"
  },
  "ava": {
    "files": [
      "./**/*.test.ts"
    ],
    "typescript": {
      "rewritePaths": {
        "test/": "built/test/"
      },
      "compile": false
    }
  }
}
