{
	"include": ["./src/**/*", "./test/**/*"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"#free-stock-tickers/*": ["src/*"],
			"#test/*": ["test/*"],
			"cheerio": ["./node_modules/cheerio/lib/index.d.ts"]
		},
		"outDir": "./built",
		"allowJs": true,
